            "   tm1-chart-type=\"${1:discreteBar}\"",
            "   tm1-height=\"${2:300}\"",
            "   tm1-axis-label-x=\"${3:}\"",
            "   tm1-axis-label-y=\"${4:}\"",
            "   tm1-data-decimal=\"${5:0}\"",
            "   tm1-duration=\"${6:2000}\"",
            "   tm1-margin=\"${7:{top: 10, right: 10, bottom: 20, left: 10}}\"",
            "   >",
            "",
            "       <tm1-ui-chart-dbr tm1-instance=\"${8:Instance Name}\" tm1-cube=\"${9:Cube Name}\" tm1-label=\"${10:Label}\" tm1-elements='${14:Comma separated element list}' ></tm1-ui-chart-dbr>",
            "       <tm1-ui-chart-dbr tm1-instance=\"${8:Instance Name}\" tm1-cube=\"${9:Cube Name}\" tm1-label=\"${11:Label}\" tm1-elements='${15:Comma separated element list}' ></tm1-ui-chart-dbr>",
